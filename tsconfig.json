{
    "compilerOptions": {
      "target": "ES2022",
      "lib": ["ES2022", "DOM", "DOM.Iterable"],
      "module": "ESNext",
      "moduleResolution": "Bundler",
      "jsx": "react-jsx",
  
      "strict": true,
      "noEmit": true,
      "skipLibCheck": true,
  
      "baseUrl": ".",
      "paths": {
        "@/*": ["resources/js/*"]
      },
  
      "types": ["vite/client"]
    },
    "include": ["resources/js/**/*.ts", "resources/js/**/*.tsx", "vite.config.*"],
    "exclude": ["node_modules"]
  }
  